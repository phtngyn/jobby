{
  "name": "nuxt",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.15.0",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "npx eslint .",
    "typecheck": "nuxt typecheck",
    "db:push": "pnpm exec drizzle-kit push",
    "db:pull": "pnpm exec drizzle-kit pull",
    "db:migrate": "pnpm exec drizzle-kit migrate",
    "db:generate": "pnpm exec drizzle-kit generate"
  },
  "devDependencies": {
    "@ai-sdk/google": "^2.0.8",
    "@ai-sdk/vue": "^2.0.22",
    "@antfu/eslint-config": "^5.2.1",
    "@iconify-json/lucide": "^1.2.63",
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/ui": "4.0.0-alpha.0",
    "@nuxtjs/mdc": "^0.17.2",
    "@pinia/nuxt": "0.11.2",
    "@types/html-to-text": "^9.0.4",
    "@types/pg": "^8.15.5",
    "@vueuse/core": "^13.7.0",
    "@vueuse/nuxt": "^13.7.0",
    "ai": "^5.0.22",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "eslint": "^9.34.0",
    "eslint-plugin-format": "^1.0.1",
    "html-to-text": "^9.0.5",
    "nano-staged": "^0.8.0",
    "nuxt": "^4.0.3",
    "pg": "^8.16.3",
    "pinia": "^3.0.3",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.2",
    "vue-tsc": "^3.0.6",
    "zod": "^4.1.0"
  },
  "pnpm": {
    "overrides": {
      "@nuxt/kit": "^4.0.3",
      "typescript": "^5.9.2",
      "vite": "^7.1.3",
      "vue": "^3.5.19",
      "vue-router": "^4.5.1",
      "zod": "^4.1.0"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "simple-git-hooks"
    ],
    "ignoredBuiltDependencies": [
      "@parcel/watcher",
      "vue-demi"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx nano-staged"
  },
  "nano-staged": {
    "*": "npx eslint . --fix"
  }
}
