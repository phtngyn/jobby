{
  "name": "nuxt",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && npx simple-git-hooks",
    "lint": "npx eslint .",
    "typecheck": "nuxt typecheck"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.1.0",
    "@nuxt/eslint": "^1.7.1",
    "@nuxt/ui": "^3.3.0",
    "@vueuse/core": "^13.6.0",
    "@vueuse/nuxt": "^13.6.0",
    "eslint": "^9.32.0",
    "eslint-plugin-format": "^1.0.1",
    "nano-staged": "^0.8.0",
    "nuxt": "^4.0.2",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.2",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.5"
  },
  "pnpm": {
    "overrides": {
      "typescript": "^5.9.2",
      "vite": "^7.0.6"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "npx nano-staged"
  },
  "nano-staged": {
    "*": "npx eslint . --fix"
  }
}
